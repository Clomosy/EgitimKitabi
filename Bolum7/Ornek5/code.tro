var  
  Matris ;  
  i, j: Integer; 
  Tut : String;
{
  Tut = '';
  Matris = VarArrayCreate([0,2,0,2],12);  
  for (i = 0 to 2) 
    for (j = 0 to 2)      
      Matris[i, j] = (i + 1) * (j + 1);  
    ShowMessage('Matrisin Elemanları:');  
  
  for (i = 0 to 2)  
  {    
    ShowMessage(IntToStr(i+1)+'. satır verileri');
    for (j = 0 to 2)      
      Tut = Tut +' '+IntToStr(Matris[i, j])
    ShowMessage (Tut);
    Tut = '';
  }
}
